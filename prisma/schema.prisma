// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quiz {
  id String @id @default(cuid())
}

model Question {
  id String @id @default(cuid())
}

model Answer {
  id String @id @default(cuid())
}

model QuizSubmission {
  id String @id @default(cuid())
}

model User {
  id String @id @default(cuid())
}
